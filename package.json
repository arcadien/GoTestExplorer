{
    "name": "gotestexplorer",
    "displayName": "Go Test Explorer",
    "description": "Go Test Explorer",
    "version": "0.0.1",
    "publisher": "prem",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:testExplorer" ,      
        "onView:fileExplorer"       
    ],
    "main": "./out/extension",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "goTestExplorer",
                    "title": "go Test Explorer",
                    "icon": "media/test.svg"
                }
            ]
        },
        "views": {
            "goTestExplorer": [
                {
                    "id": "testExplorer",
                    "name": "Go Test Explorer"
                }
                
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mkdirp": "^0.5.2",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "@types/rimraf": "^2.0.2",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    },
    "dependencies": {
        "mkdirp": "^0.5.1",
        "rimraf": "^2.6.2"
    }
}
